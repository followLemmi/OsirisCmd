<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewModels="clr-namespace:Application.ViewModels;assembly=Application"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Application.Views.SettingsWindow"
        Title="SettingsWindow"
        Width="1024" Height="780"
        x:DataType="viewModels:SettingsWindowViewModel">

    <Window.DataContext>
        <viewModels:SettingsWindowViewModel />
    </Window.DataContext>

    <TabControl TabStripPlacement="Left" ItemsSource="{Binding Tabs}">   
    </TabControl>

</Window>
